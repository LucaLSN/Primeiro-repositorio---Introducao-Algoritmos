algoritmo "Conteate10"
var
   contador, valor: inteiro
inicio
    contador <-0
    Escreva("Até qual número gostaria de contar?")
    Leia(valor)
    Enquanto (contador <= valor) faca
    EscrevaL(contador)
              contador <- contador + 1
    FimEnquanto
    EscrevaL("Terminei de contar")
fimalgoritmo