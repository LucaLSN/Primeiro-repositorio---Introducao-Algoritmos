algoritmo "Contador 0 a 10"
var
     contador : inteiro
inicio
      contador <-10
      // Enquanto (contador <= 10) e (contador >= 0) faca
      Enquanto (contador >= 0) faca
      EscrevaL(contador)
               contador <- contador - 1
      FimEnquanto
      EscrevaL("Terminei de contar!")
fimalgoritmo